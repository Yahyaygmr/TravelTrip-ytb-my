@using TravelTrip.Models.Siniflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_ApplicationLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>About</h3>
            @foreach (var item in Model)
            {
                <img src="@item.FotoUrl" alt="" />
                <p>
                    @item.Aciklama
                </p>
            }

        </div>
    </div>
</div>

